<div class="table-responsive">
{{#if productos}}
        <table class="table table-dark">
            <tr style="color: yellow">
                <th>Nombre</th>
                <th>Precio</th>
                <th>Imagen</th>
            </tr>
            {{#each productos}}
             <tr>
                <td style="vertical-align:middle">{{@nombre}}{{nombre}}</td>
                <td style="vertical-align:middle">{{@precio}}{{precio}}</td>
                <td>
                    <img src={{@url}}{{url}} height="40px" width="40px"></img>
            </tr>
            {{/each}}
        </table>
    </div>
{{else}}
    <h3 class="alert alert-danger"> No se encontraron datos </h3>
{{/if}}
<a class="btn btn-back btn-success mt-3 mb-5" style="margin: 10px;" href="/">Volver al formulario</a>     